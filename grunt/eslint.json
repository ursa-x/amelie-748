{
	"extends": "airbnb-base",
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"no-tabs": "off",
		"one-var": [
			"warn",
			{
				"initialized": "always"
			}
		],
		"comma-dangle": [
			"error",
			"never"
		],
		"class-methods-use-this": "off",
		"import/no-useless-path-segments": "off"
	}
}
